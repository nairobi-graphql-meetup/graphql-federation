{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "quotes": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "no-underscore-dangle": "off",
    "no-console": "off"
  }
}
